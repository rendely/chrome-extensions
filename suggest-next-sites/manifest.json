{
  "manifest_version": 3,
  "name": "Suggest Next Sites",
  "description": "Show suggested next sites based on ihstory",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "history", "sidePanel"],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_title": "Click to open panel"
  }
}